    "uuid": "^8.3.2",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.7",
    "@remix-run/eslint-config": "^1.6.7",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.20.0",
    "typescript": "^4.7.4",
    "@savvywombat/tailwindcss-grid-areas": "^2.0.1",
    "@types/uuid": "^8.3.4",
